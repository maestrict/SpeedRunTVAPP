<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SpeedRunTV2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:SpeedRunTV2.View"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
                Title="Game"
                ContentTemplate="{DataTemplate view:MainPage}"
                Route="MainPage"
                Icon="product.png"/>
        <ShellContent
                Title="PersoPage"
                ContentTemplate="{DataTemplate view:Setting}"
                Route="MaintPage2"
                Icon="settings.png"/>


    </TabBar>
    

</Shell>

